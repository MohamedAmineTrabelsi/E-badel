<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="468.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml" fx:controller="com.ebadel.gui.VideoController">
   <children>
      <MediaView fx:id="mediaview" fitHeight="300.0" fitWidth="700.0" layoutX="59.0" layoutY="-20.0" />
      <Button fx:id="playButton" layoutX="143.0" layoutY="402.0" mnemonicParsing="false" onAction="#playMedia" text="Play" />
      <Button fx:id="pauseButton" layoutX="342.0" layoutY="415.0" mnemonicParsing="false" onAction="#pauseMedia" text="Pause" />
      <Button fx:id="resetButton" layoutX="495.0" layoutY="415.0" mnemonicParsing="false" onAction="#resetMedia" text="Reset" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="559.0" layoutY="318.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
